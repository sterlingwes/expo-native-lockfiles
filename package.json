{
  "name": "expo-native-lockfiles",
  "version": "0.1.0",
  "main": "app.plugin.js",
  "repository": "https://github.com/sterlingwes/expo-native-lockfiles",
  "author": "Wes Johnson <wes@swj.io>",
  "license": "MIT",
  "bin": {
    "native-lock": "./cli/native-lock.sh"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "expo": "~51.0.28",
    "typescript": "^5"
